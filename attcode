hp = dado_vida
d6vida = [4,5,6]
d8vida = [5,6,7,8]
d10vida = [6,7,8,9,10]
d12vida =[7,8,9,10,11,12]
for num in range(level):
    if dado_vida == 6:
        hp += random.choice(d6vida)
    elif dado_vida == 8:
        hp += random.choice(d8vida)
    elif dado_vida == 10:
        hp += random.choice(d10vida)
    elif dado_vida == 12:
        hp += random.choice(d12vida)
d6 = [1,2,3,4,5,6]
