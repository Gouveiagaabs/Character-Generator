print("HP = " + str(hp))
dadosEscolhidos = []
for num in range(6):
        resultadod6 = []
        for index in range(4):
                resultadod6.append(random.choice(d6))
        resultadod6.remove(min(resultadod6))
        soma = sum(resultadod6)
        dadosEscolhidos.append(soma)
def printdado():
    print("Força = " + str(dadosEscolhidos[0]))
    print("Destreza = " + str(dadosEscolhidos[1]))
